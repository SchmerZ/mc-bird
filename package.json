{
  "name": "mc-bird",
  "version": "1.0.0",
  "description": "MessageBird Assignment",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start:server": "nodemon --watch ./src/server ./src/server/index.babel.js --mcBird:Environment development",
    "start:client": "webpack-dev-server --config ./webpack/webpack.development.config --mode development --hot --inline --color",
    "build:server": "babel --copy-files src/server -d dist/server && babel --copy-files src/shared -d dist/shared",
    "build:client": "webpack --config ./webpack/webpack.production.config --mode production --progress --color",
    "build": "npm run build:client && npm run build:server",
    "start:server-prod": "cross-env NODE_ENV=production nodemon --watch ./dist/server ./dist/server/index.js --mcBird:Environment production",
    "dev:storybook": "start-storybook -p 9001 -c .storybook",
    "start:tunnel": "lt --port 8090",
    "test:jest": "jest --runInBand --config tests/config/jest.config.js --watch --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SchmerZ/mc-bird.git"
  },
  "author": "Vadim Sheydakov",
  "homepage": "https://github.com/SchmerZ/mc-bird#readme",
  "dependencies": {
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "connected-react-router": "4.4.1",
    "express": "4.16.3",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "moment": "2.22.2",
    "nconf": "0.10.0",
    "npm": "^6.4.0",
    "on-finished": "2.3.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-transition-group": "2.4.0",
    "redux": "4.0.0",
    "redux-actions": "2.6.1",
    "redux-devtools-extension": "2.13.5",
    "redux-saga": "0.16.0",
    "redux-saga-routines": "2.2.5",
    "reselect": "3.0.1",
    "uncontrollable": "6.0.0",
    "websocket": "1.0.26",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.14",
    "@storybook/addon-info": "4.0.0-alpha.14",
    "@storybook/react": "4.0.0-alpha.14",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-jest": "23.4.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "nodemon": "1.18.3",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "copy-webpack-plugin": "4.5.2",
    "styled-components": "3.4.2",
    "localtunnel": "1.9.0",
    "jest": "23.5.0",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.3.0",
    "enzyme-to-json": "3.3.4",
    "cross-env": "5.2.0"
  }
}
